@page
@model Travellingfront.Pages.Admins.Countries.DeleteModel
@{
}
<head>
    <link href="~/css/Admin/admin.css" rel="stylesheet" />
    <link href="~/css/Admin/all.css" rel="stylesheet" />
    <link href="~/css/Admin/style.css" rel="stylesheet" />

</head>
<div class="text-center">

    <div class="banner" style="background-image:url('../../adminbg.jpg');">
        <div class="container">
            <div class="banner-text">
                <div class="banner-heading">
                    حذف @Model.country.Name
                </div>
            </div>
        </div>
    </div>

    <div class="body-of-admin">
        <div class="container" style="direction: rtl;">
            <div class="row">
                <div class="col-lg-3 col-md-12">
                    <div class="account-info-box fullsize panel-boxes text-right">
                        <ul class="account-nav fullsize">
                            <li>
                                <a asp-page="../Places/Place">
                                    <i class='far fa-map'></i>
                                    جاذبه های گردشگری
                                </a>
                            </li>
                            <li>
                                <a asp-page="../Categories/Category">
                                    <i class='fas fa-layer-group'></i>
                                    دسته بندی
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <i class='fas fa-flag'></i>
                                    کشور ها
                                </a>
                            </li>
                            <li>
                                <a href="cities.html">
                                    <i class='fas fa-map-marker-alt'></i>
                                    شهر ها
                                </a>
                            </li>
                            <li>
                                <a href="comments.html">
                                    <i class='far fa-comment-alt'></i>
                                    نظرات
                                </a>
                            </li>
                            <li>
                                <a href="rates.html">
                                    <i class='fas fa-star'></i>
                                    رای ها
                                </a>
                            </li>
                            <li>
                                <a href="users.html">
                                    <i class='fas fa-id-card'></i>
                                    کاربران
                                </a>
                            </li>
                            <li>
                                <a href="userlogins.html">
                                    <i class='fas fa-user-clock'></i>
                                    لاگین ها
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-9 col-md-12">
                   @if (Model.State!=true)
                   {
                    <p>@ViewBag.Error</p>
                    <p>@Model.country.Name را نمی توان حذف کرد</p>
                   }
                  
                </div>
            </div>
        </div>
    </div>
</div>